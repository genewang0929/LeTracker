{
  "manifest_version": 3,
  "name": "LeTracker Sync",
  "version": "1.0",
  "description": "Sync LeetCode history to LeTracker",
  "permissions": ["cookies", "activeTab", "scripting"],
  "host_permissions": ["https://leetcode.com/*", "http://localhost:8080/*"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Sync LeetCode"
  }
}
